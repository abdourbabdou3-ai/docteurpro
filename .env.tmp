# Database
# For Docker, use: postgresql://postgres:password@db:5432/docteur
DATABASE_URL="postgresql://postgres:password@localhost:5432/docteur"

# Postgres Container Settings (Only used by docker-compose)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=docteur

# Next Auth
# Generate one with: openssl rand -base64 32
NEXTAUTH_SECRET="your-secret-key-here"
NEXTAUTH_URL="http://localhost:3000"

# Email
EMAIL_PASSWORD="your-email-password"
